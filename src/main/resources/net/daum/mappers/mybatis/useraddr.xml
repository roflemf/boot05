<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="UserAddr"> <!-- 매퍼 네임스페이스명을 UserAddr로 지정 -->
	
	<!-- 이름,주소 저장 -->
	<insert id="useraddr_in" parameterType="userAddr">
	<!-- parameterType은 전달인자 타입이고 생략가능. userAddr은 mybatis-config.xml에서 설정한 UserAddrVO.java 빈클래스 객체 별칭이름-->
		
	  insert into tbl_useraddr2 (userno,username,useraddr,regdate)
	  values(bno_seq2.nextval,#{username},#{useraddr},sysdate)
	</insert>
</mapper>

